              <script>

                async function register_otp() {
                  if (validateForm()) {
                      try {
                          const response = await saveFormData();
  
                          if (response.error) {
                              console.error('Server Error:', response.error);
                              displayErrorMessag(response.error);
                          } else if (response.success) {
                              console.log('Registration successful:', response);
                              openOTPModal();
                          }
  
                      } catch (error) {
                          console.error('Error during registration:', error);
                          displayErrorMessag('Error during registration. Please try again.');
                      }
                  }
                }
                
                function displayErrorMessag(message) {
                  const errorMessageContainer = document.getElementById('error-message-container');
  
                  if (!errorMessageContainer) {
                      const container = document.createElement('div');
                      container.id = 'error-message-container';
                      document.body.appendChild(container);
                  }
  
                  const errorMessageElement = document.createElement('p');
                  errorMessageElement.className = 'modal-body';
                  errorMessageElement.style.color = 'red';
                  errorMessageElement.textContent = message;
  
                  errorMessageContainer.appendChild(errorMessageElement);
                }
  
                function openOTPModal() {
                  $('#otpModal').modal('show');
                  
                  const resendOTPButton = document.getElementById('resendOTPButton');

                  startOTPTimer(60, resendOTPButton);

                }
                
                function getReferralCodeFromURL() {
                    const urlParams = new URLSearchParams(window.location.search);
                    const referralCode = urlParams.get('ref');
                    return referralCode;
                }

                function saveFormData() {
                  return new Promise((resolve, reject) => {
                  const referral_code = getReferralCodeFromURL();

                        var formData = {
                            name: $('#firstname').val(),
                            email: $('#email-login').val(),
                            mno: $('#telephone').val(),
                            password: $('#password-login').val(),
                            cpassword: $('#password-confirm').val(),
                        };

                        if (referral_code) {
                            formData.referralCode = referral_code;
                        }

                        $.ajax({
                            url: '/register',
                            method: 'POST',
                            data: formData, 
                            success: function (response) {
                                console.log('Form data saved successfully!', response);
                                resolve(response);
                            },
                            error: function (error) {
                                console.error('Error saving form data:', error);
                                reject(error);
                            }
                        });
                    });
                }
  
                function verifyOTP() {
                    var enteredOTP = $('#otpInput').val();
                    var email = $('#email-login').val();

                    $.ajax({
                        url: '/verify-otp',
                        method: 'POST',
                        data: { otp: enteredOTP, email: email },
                        success: function (response) {
                            if (response.success) {
                                console.log('OTP verification successful!');
                                $('#otpModal').modal('hide');
                                displaySuccessMessage(response.message)
                            } else {
                                console.error('OTP verification failed:', response.error);
                                displayError(response.error);
                            }
                        },
                        error: function (error) {
                            console.error('Error during OTP verification:', error);
                            displayError(response.error);
                        }
                    });
                }
                
                function displayError(message) {
                      const errorMessageContainer = document.getElementById('otp-error');

                      if (!errorMessageContainer) {
                          const container = document.createElement('div');
                          container.id = 'otp-error';
                          document.body.appendChild(container);
                      } else {
                          errorMessageContainer.innerHTML = '';
                      }

                      const errorMessageElement = document.createElement('p');
                      errorMessageElement.className = 'error-message';
                      errorMessageElement.style.color = 'red';
                      errorMessageElement.textContent = message;

                      errorMessageContainer.appendChild(errorMessageElement);
                  }
               
                function startOTPTimer(seconds, resendOTPButton) {
                    const timerElement = document.getElementById('timer');

                    let remainingTime = seconds;

                    function updateTimer() {
                        const minutes = Math.floor(remainingTime / 60);
                        const seconds = remainingTime % 60;

                        timerElement.textContent = `${minutes}:${seconds < 10 ? '0' : ''}${seconds}`;

                        if (remainingTime === 0) {
                            displayError('OTP has expired. Please resend.');
                            
                            resendOTPButton.disabled = false;
                        } else {
                            remainingTime--;
                            setTimeout(updateTimer, 1000);
                        }
                    }
                    resendOTPButton.disabled = true;

                    updateTimer();
                }
                  
                function resendOTP()  {
                  var email = $('#email-login').val() ;
                  $.ajax({
                    url: '/resend-otp',
                    method: 'POST',
                    data: {email},
                    success: function (response) {
                        console.log('Resend OTP request successful!', response);
                        clearErrorMessages();
                        const resendOTPButton = document.getElementById('resendOTPButton');
                        startOTPTimer(60, resendOTPButton);
                    },
                    error: function (error) {
                        console.error('Error sending resend OTP request:', error);
                }
                  })
                }

                function displaySuccessMessage(message) {
                      Swal.fire({
                          icon: 'success',
                          title: 'Verification Successful!',
                          text: "Continue With Login",
                          showConfirmButton: false,
                          timer: 15000, 
                          customClass: {
                              popup: 'sweetalert-custom-popup',
                              title: 'sweetalert-custom-title',
                              content: 'sweetalert-custom-content',
                          },
                          background: '#f8f9fa', // Set background color
                          iconColor: '#28a745', // Set icon color
                          timerProgressBar: true, // Display a progress bar during the time
                      });
                  }

                function validateForm() {
                    var form = document.querySelector('.login');
                    var valid = true;
  
                    var errorMessages = form.querySelectorAll('.error-message');
                    errorMessages.forEach(function (errorMessage) {
                        errorMessage.remove();
                    });
  
                    var fullName = form.querySelector('#firstname');
                    if (!fullName.value.trim()) {
                        displayErrorMessage(fullName, 'Full Name is required');
                        valid = false;
                    } else {
                        var namePattern = /^[a-zA-Z\s]+$/;

                        if (fullName.value.length < 2 || fullName.value.length > 15 || !namePattern.test(fullName.value)) {
                            displayErrorMessage(fullName, 'Enter a valid full name (2-15 characters only)');
                            valid = false;
                        }
                    }

  
                    var email = form.querySelector('#email-login');
                    var emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                    if (!emailRegex.test(email.value.trim())) {
                        displayErrorMessage(email, 'Invalid Email format');
                        valid = false;
                    }
                    
                    var telephone = form.querySelector('#telephone');
                    if (!telephone.value.trim()) {
                        displayErrorMessage(telephone, 'Telephone is required');
                        valid = false;
                    } else {
                        // Pattern for allowing only numeric characters
                        var numericPattern = /^\d+$/;

                        if (!numericPattern.test(telephone.value)) {
                            displayErrorMessage(telephone, 'Enter a valid numeric telephone number');
                            valid = false;
                        }
                    }
  
                    var password = form.querySelector('#password-login');
                    if (!password.value.trim()) {
                        displayErrorMessage(password, 'Password is required');
                        valid = false;
                    } else if (password.value.length < 5 || !/\d/.test(password.value) || !/[a-zA-Z]/.test(password.value)) {
                        displayErrorMessage(password, 'Give a strong password.');
                        valid = false;
                    }

                    var confirmPassword = form.querySelector('#password-confirm');
                    if (!confirmPassword.value.trim()) {
                        displayErrorMessage(confirmPassword, 'Password is required');
                        valid = false;
                    }

