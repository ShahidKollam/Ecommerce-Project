                              <label for="telephone">Telephone <span class="require">*</span></label>
                              <input type="tel" id="telephone" name="mno" class="form-control" placeholder="Your Mobile" required>
                          </div>

                          <h3>Your Password</h3>

                          <div class="form-group">
                              <label for="password-login">Password <span class="require">*</span></label>
                              <input type="password" id="password-login" class="form-control" name="password" placeholder="Password" required>
                          </div>
                          <div class="form-group">
                              <label for="password-confirm">Confirm Password <span class="require">*</span></label>
                              <input type="password" id="password-confirm" class="form-control" name="cpassword" placeholder="Confirm Password" required>
                          </div>
                          <p> Give at least 5 characters long and include both letters and numbers.</p>

                          <div class="col-md-12">
                            
                            <div id="error-message-container"></div>

                              <% if (typeof message !== 'undefined') { %> <p style="color: green;"><%= message %></p> <% } %>
                              <% if (typeof error !== 'undefined') { %>  <p style="color: red"><%= error %></p> <% } %>

                              <div class="padding-top-20">
                                <button class="btn btn-primary pull-right" type="button" id="button-payment-address" onclick="register_otp()">Send OTP</button>  
                              </div>
